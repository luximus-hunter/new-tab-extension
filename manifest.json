{
    "manifest_version": 2,
    "name": "New Tab V3",
    "version": "0.0.1",
    "description": "A plain text description",
    "permissions": [
        "bookmarks",
        "activeTab",
        "storage",
        "https://t0.gstatic.com/*"
    ],
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "options_ui": {
        "page": "settings.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "newtab@luximus.eu",
            "strict_min_version": "42.0"
        }
    } 
}