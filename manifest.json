{
    "manifest_version": 2,
    "name": "New Tab V3",
    "version": "0.0.1",
    "description": "A plain text description",
    "permissions": [
        "bookmarks",
        "activeTab",
        "https://t0.gstatic.com/*"
    ],
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "browser_action": {  
        "default_icon": "tab-icon.png",   
        "default_title": "Have a good day" 
    },
    "content_security_policy": "script-src 'self' https://t0.gstatic.com; object-src 'self'"
  }