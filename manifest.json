{
    "manifest_version": 2,
    "name": "NewTab Bookmarks",
    "version": "1.0.0",
    "description": "Visualize your bookmarks in a new tab",
    "author": "Luximus",
    "permissions": [
        "bookmarks",
        "activeTab",
        "storage",
        "unlimitedStorage",
        "https://t0.gstatic.com/*"
    ],
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "options_ui": {
        "page": "settings.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "newtab@luximus.eu",
            "strict_min_version": "42.0"
        }
    },
    "omnibox": {
        "keyword": "b"
    },
    "background": {
        "scripts": ["search.js"]
      }
}